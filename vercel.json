{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/market",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/market/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/market/index.html"
    }
  ]
}
